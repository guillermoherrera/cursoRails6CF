<%= form_with(model:@article, local: true) do |form|%>
  <div class='field'>
    <%=form.label :titulo, "Titulo"%>
    <%=form.text_field :titulo%>
  </div>

  <div class='field'>
    <%=form.label :contenido, "Contenido"%>
    <%=form.rich_text_area :contenido%>
  </div>

  <div>
    <%=form.submit%>
  </div>    
<%end%>
